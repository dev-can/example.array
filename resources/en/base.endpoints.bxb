endpoints {
  action-endpoints {
    action-endpoint (ActionGetContactByName) {
      accepted-inputs (name)
      local-endpoint ("GetContactByName.js")
    }
    action-endpoint (ActionCallContact) {
      accepted-inputs (contact)
      local-endpoint ("CallContact.js")
    }
    // add action-endpoint here 
    action-endpoint (ActionConferenceCall) {
      accepted-inputs (contacts)
      local-endpoint ("ConferenceCall.js")
    }
  }
}
